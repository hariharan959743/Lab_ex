public class Room {
    double length, width;

    public Room(double length, double width) {
        this.length = length;
        this.width = width;
    }

    public double calculateArea() {
        return length * width;
    }

    public static void main(String[] args) {
        Room room = new Room(5.0, 4.0);
        System.out.println("Area of the room: " + room.calculateArea());
    }
}
